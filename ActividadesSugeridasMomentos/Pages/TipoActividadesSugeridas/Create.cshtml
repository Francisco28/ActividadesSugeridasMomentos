@page
@model ActividadesSugeridasMomentos.Pages.TipoActividadesSugeridas.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h2>Crear Nuevo Tipo de Actividad Sugerida</h2>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="eva_cat_tipo_actividades_sugeridas.DesTipoActividadSug" class="control-label">Descripción de Tipo Actividad</label>
                <input role="button" asp-for="eva_cat_tipo_actividades_sugeridas.DesTipoActividadSug" class="form-control" />
                <span asp-validation-for="eva_cat_tipo_actividades_sugeridas.DesTipoActividadSug" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input class="btn btn-success btn-md active" type="submit" value="Crear" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
